# Example .env for Memex project (DO NOT COMMIT REAL KEYS)
# Ethereum
PRIVATE_KEY=your_ethereum_private_key_here_without_0x_prefix
ETH_RPC_URL=https://sepolia.infura.io/v3/YOUR_INFURA_PROJECT_ID
MEMEX_CONTRACT_ADDRESS=0xYourDeployedContractAddressHere

# CoinGecko (optional)
COINGECKO_API_URL=https://api.coingecko.com/api/v3/coins/markets

# Indexer settings
INDEX_THRESHOLD_BPS=200
MAX_ADJUST_BPS=500

# Solana
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_KEYPAIR_PATH=./solana-keypair.json
SOLANA_MINT_ADDRESS=YourSPLMintPublicKeyHere
